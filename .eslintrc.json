{
    "extends": "airbnb-base",
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [ "error",
        "unix"
        ],
        "quotes": ["error", "single"],
        "max-len": 0,
        "no-multi-spaces":"off",
        "class-methods-use-this": "off"
    },
    "globals": {
        "browser": true
    }
}
